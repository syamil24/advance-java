<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-4.3.xsd">

    <bean id="user" class="com.maybank.dto.core.User" destroy-method="destroy" init-method="init" >
        <!-- User user = new User(); user.init(); user.destroy() -->
        <property name="name" value="Syamil"></property> <!-- user.setName("Kumar"-->
    </bean>

    <bean id="Login" class="com.maybank.dto.core.Login" destroy-method="destroy" init-method="initialization" >
        <!-- User user = new User(); user.init(); user.destroy() -->
        <property name="username" value="Syamil"></property> <!-- user.setName("Kumar"-->
        <property name="password" value="Pass123"></property>
        <property name="email" value="syamil@test.com"></property>
    </bean>

</beans>
